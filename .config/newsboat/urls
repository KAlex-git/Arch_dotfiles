#vim ~/.config/newsboat/urls

# -------- Query Feeds {{{
# ------------------------------------------------------
# query feeds
# query:<name of feed>:<filter expression> [<tag> ...]
#
# ------------------------------------------------------
# https://newsboat.org/releases/2.11.1/docs/newsboat.html#_filter_language
# https://newsboat.org/releases/2.10.2/docs/newsboat.html
#"query:Only 720p:tags =~ \"BTRSS\" and ( title =~ \"720p\" )" "BTRSS"
#"query:Only 1080p:tags =~ \"BTRSS\" and ( title =~ \"1080p\" )" "BTRSS"
#"query:Only SD:tags =~ \"BTRSS\" and ( title =~ \"hdtv|x264|web\" )" "BTRSS"
# ------------------------------------------------------
"query:Study - Октябрь:tags =~ \"Study_wU_MP3\" and ( content =~ \" Октябрь\" )" "Study_wU_MP3"
"query:Study -Ноябрь:tags =~ \"Study_wU_MP3\" and ( content =~ \"Ноябрь\" )" "Study_wU_MP3"
"query:Study -Декабрь:tags =~ \"Study_wU_MP3\" and ( content =~ \"Декабрь\" )" "Study_wU_MP3"
"query:Study -Январь:tags =~ \"Study_wU_MP3\" and ( content =~ \"Январь\" )" "Study_wU_MP3"
# ------------------------------------------------------
"query:Study_wU:tags # \"Study_wU_MP3\"" "Study_wpU_MP3"
# }}}



